    <div class="col-md-8 offset-md-1">
      <div class="py-4">
        <h3>会員一覧</h3>
      </div>
    <table class="table table-hover table-inverse">
  <thead>
    <tr>
     <th>ユーザーID</th>
     <th>名前</th>
     <th>メールアドレス</th>
     <th>ステータス</th>
     <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
　　<% @users.each do |user| %>
       <tr>
         <td>
           <%= user.id %>
         </td>
         <td>
          <%= link_to admins_user_path(user) do %>
          <%= user.name %>
          <% end %>
         </td>
         <td>
           <%= user.email %>
         </td>
         <td>
           <% if user.is_active == true %>
           <p>有効</p>
           <% elsif user.is_active == false %>
           <p>退会</p>
           <% end %>
         </td>
       </tr>
      <% end %>
    </tbody>
   </table>
    <div class="mb-5"><%= paginate(@users) %></div>
   </tbody>
  </table>
    </div>
  </div>
